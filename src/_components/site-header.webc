<header class="main-nav">
	<a webc:if="page.url == '/'" :aria-current="page" class="current-page" @raw="metadata.title"></a>
	<a webc:else href="/" @raw="metadata.title"></a>

	<nav>
	  <ul webc:for="item of $data.collections.nav">
	    <a webc:if="item.url == page.url" :aria-current="page" class="current-page" @raw="item.data.title"></a>
	    <a webc:else :href="item.url" @raw="item.data.title"></a>
	  </ul>
	</nav>
</header>
